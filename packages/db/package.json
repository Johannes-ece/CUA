    "db-push": "yarn with-env drizzle db push",
    "db-generate": "yarn with-env drizzle generate"
  },
  "dependencies": {
    "drizzle-orm": "^0.27.0",
    "postgres": "^3.3.5"
  },
  "devDependencies": {
    "dotenv-cli": "^6.0.0",
    "drizzle-kit": "^0.19.2",
    "typescript": "4.9.4"
  }
